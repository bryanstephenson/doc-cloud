        <desc desctype="option" domain="oslo.config" noindex="False" objtype="option">
            <desc_signature allnames="handle_internal_only_routers" first="False" ids="DEFAULT.handle_internal_only_routers"><desc_name xml:space="preserve">handle_internal_only_routers</desc_name></desc_signature>
            <desc_content>
                <field_list>
                    <field>
